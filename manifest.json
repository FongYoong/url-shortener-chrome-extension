{
    "name": "URL Shortener",
    "version": "1.0",
    "description": "Shortens URL using a custom API",
    "permissions": [
      "https://ushnk.herokuapp.com/",
      "clipboardWrite",
      "clipboardRead",
      "tabs",
      "activeTab",
      "declarativeContent"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "options_page": "options.html",
    "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16": "images/shrink16.ico",
        "32": "images/shrink32.ico",
        "48": "images/shrink48.ico",
        "64": "images/shrink64.ico",
        "128": "images/shrink128.ico"
      },
    "default_title":"URL Shortener"
    },
    "icons": {
        "16": "images/shrink16.ico",
        "32": "images/shrink32.ico",
        "48": "images/shrink48.ico",
        "64": "images/shrink64.ico",
        "128": "images/shrink128.ico"
      },
    "manifest_version": 2
  }